{
    "_beagleAction_": "beagle:alert",
    "analytics": {
        "attributes": [
            "labelOk",
            "message",
            "onPressOk.message",
            "title"
        ],
        "additionalEntries": {
            "labelOk": "additionalEntries takes precedence"
        },
        "enable": true
    },
    "labelOk": "ok",
    "message": "Message - @{context.headers.pi}",
    "onPressOk": {
        "_beagleAction_": "beagle:alert",
        "message": "@{context.query}"
    },
    "title": "Title - @{context.headers.platform}"
}