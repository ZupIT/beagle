{
    "_beagleAction_": "beagle:confirm",
    "analytics": {
        "attributes": [
            "labelCancel",
            "labelOk",
            "message",
            "onPressCancel.message",
            "onPressOk.message",
            "title"
        ],
        "enable": true
    },
    "labelCancel": "👎🏻",
    "labelOk": "👍🏻",
    "message": "Message - @{context.headers.pi}",
    "onPressCancel": {
        "_beagleAction_": "beagle:alert",
        "message": "Cancel - @{context.query}"
    },
    "onPressOk": {
        "_beagleAction_": "beagle:alert",
        "message": "OK - @{context.query}"
    },
    "title": "Title - @{context.headers.platform}"
}