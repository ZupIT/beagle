{
    "_beagleAction_": "beagle:condition",
    "analytics": {
        "attributes": [
            "condition",
            "onFalse",
            "onTrue"
        ],
        "enable": true
    },
    "condition": "@{eq(context.query, 'beagle')}",
    "onFalse": [
        {
            "_beagleAction_": "beagle:alert",
            "message": "Hello @{context.query}"
        }
    ],
    "onTrue": [
        {
            "_beagleAction_": "beagle:alert",
            "message": "Hello @{context.query}"
        }
    ]
}
